#!/bin/bash
/usr/local/cuda-7.5/bin/nvcc -o srs memCompSRSL.cu -arch=sm_52


./srs 256 256 100 && ./srs 256 256 100 && ./srs 256 256 100
./srs 256 256 500 && ./srs 256 256 500 && ./srs 256 256 500
./srs 256 256 1000 && ./srs 256 256 1000 && ./srs 256 256 1000
./srs 256 256 5000 && ./srs 256 256 5000 && ./srs 256 256 5000

printf "\n"
./srs 512 512 100 && ./srs 512 512 100 && ./srs 512 512 100
./srs 512 512 500 && ./srs 512 512 500 && ./srs 512 512 500
./srs 512 512 1000 && ./srs 512 512 1000 && ./srs 512 512 1000
./srs 512 512 5000 && ./srs 512 512 5000 && ./srs 512 512 5000

printf "\n"
./srs 1024 512 100 && ./srs 1024 512 100 && ./srs 1024 512 100
./srs 1024 512 500 && ./srs 1024 512 500 && ./srs 1024 512 500
./srs 1024 512 1000 && ./srs 1024 512 1000 && ./srs 1024 512 1000
./srs 1024 512 5000 && ./srs 1024 512 5000 && ./srs 1024 512 5000

printf "\n"
./srs 1024 1024 100 && ./srs 1024 1024 100 && ./srs 1024 1024 100
./srs 1024 1024 500 && ./srs 1024 1024 500 && ./srs 1024 1024 500
./srs 1024 1024 1000 && ./srs 1024 1024 1000 && ./srs 1024 1024 1000
./srs 1024 1024 5000 && ./srs 1024 1024 5000 && ./srs 1024 1024 5000

printf "\n"
./srs 2048 2048 100 && ./srs 2048 2048 100 && ./srs 2048 2048 100
./srs 2048 2048 500 && ./srs 2048 2048 500 && ./srs 2048 2048 500
./srs 2048 2048 1000 && ./srs 2048 2048 1000 && ./srs 2048 2048 1000
./srs 2048 2048 5000 && ./srs 2048 2048 5000 && ./srs 2048 2048 5000
