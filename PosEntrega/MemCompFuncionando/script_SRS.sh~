./srs 1000 500 500 && ./srs 1000 500 500 && ./srs 1000 500 500
./srs 1000 500 1000 && ./srs 1000 500 1000 && ./srs 1000 500 1000
./srs 1000 500 5000 && ./srs 1000 500 5000 && ./srs 1000 500 5000

printf "\n"
./srs 1000 1000 100 && ./srs 1000 1000 100 && ./srs 1000 1000 100
./srs 1000 1000 500 && ./srs 1000 1000 500 && ./srs 1000 1000 500
./srs 1000 1000 1000 && ./srs 1000 1000 1000 && ./srs 1000 1000 1000
./srs 1000 1000 5000 && ./srs 1000 1000 5000 && ./srs 1000 1000 5000

