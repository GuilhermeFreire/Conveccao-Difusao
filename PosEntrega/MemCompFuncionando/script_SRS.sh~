./srs 500 500 500 && ./srs 500 500 500 && ./srs 500 500 500
./srs 500 500 1000 && ./srs 500 500 1000 && ./srs 500 500 1000
./srs 500 500 5000 && ./srs 500 500 5000 && ./srs 500 500 5000

